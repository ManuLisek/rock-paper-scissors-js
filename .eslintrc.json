{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 2015
  },
  "globals": {
    "printMessage": "readonly",
    "clearMessages": "readonly"
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single",
          {"allowTemplateLiterals": true}
      ],
      "semi": [
          "error",
          "always"
      ],
      "no-console": [
          "off"
      ]
  }
}